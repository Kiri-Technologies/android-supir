<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/account_nav"
    app:startDestination="@id/accountFragment">

    <fragment
        android:id="@+id/accountFragment"
        android:name="com.kiri.account.presentation.fragment.AccountFragment"
        android:label="@string/title_account"
        tools:layout="@layout/account_fragment" >
        <action
            android:id="@+id/action_accountFragment_to_detailAccountFragment"
            app:destination="@id/detailAccountFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailAccountFragment"
        android:name="com.kiri.account.presentation.fragment.DetailAccountFragment"
        android:label="Detail Akun"
        tools:layout="@layout/detail_account_fragment" />
</navigation>